      "source": [
        "#write a python program to calculate the sum of all even numbers between 1 and given positive integer n\n",
        "sum=0\n",
        "n=int(input(\"Enter the number:\"))\n",
        "for i in range(0,n+1):\n",
        "  if i%2==0:\n",
        "    sum+=i\n",
        "print(\"total value :\",sum)\n"
      ]
